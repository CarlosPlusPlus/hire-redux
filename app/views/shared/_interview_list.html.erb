<div class="row-fluid sortable">		
	<div class="box span12">
		<div class="box-header" data-original-title>
			<h2><i class="icon-user"></i><span class="break"></span>Interviews</h2>
			<div class="box-icon">
				<a href="#" class="btn-setting"><i class="icon-wrench"></i></a>
				<a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
				<a href="#" class="btn-close"><i class="icon-remove"></i></a>
			</div>
		</div>
		<div class="box-content">
			<table class="table table-striped table-bordered bootstrap-datatable datatable">
			  <thead>
				  <tr>
					  <th>Name</th>
					  <th>Company</th>
					  <th>Job Title</th>
					  <th>Date</th>
					  <th>Time</th>
					  <th>Status</th>
					  <th>Actions</th>
				  </tr>
			  </thead>   
			  <tbody>

			  <% if interview_obj.first.class.to_s != "Interview" %>
			  	
			  	<% interview_obj.interviews.each do |interview| %>
			  		<%= render :partial => "shared/interview_build", :locals => {:interview => interview} %>
			  	<% end %>
			  
			  <% else %>
					
					<% interview_obj.each do |interview| %>
						<%= render :partial => "shared/interview_build", :locals => {:interview => interview} %>
					<% end %>

				<% end %>

			  </tbody>

		  </table>            
		</div>
	</div><!--/span-->
